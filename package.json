    "framer-motion": "^6.0.0",
    "leaflet": "^1.9.4",
    "moment": "2.29.4",
    "next": "^12.3.1",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.4.0",
    "next-images": "1.3.1",
    "node-sass": "^9.0.0",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "path": "0.12.7",
    "postcss": "^8.4.17",
    "postcss-import": "^13.0.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-leaflet": "^4.2.1",
    "react-scroll": "^1.8.1",
    "react-slick": "^0.27.13",
    "reactstrap": "8.9.0",
